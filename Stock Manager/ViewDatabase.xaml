        <Button x:Name="PrintDB" Content="Print" HorizontalAlignment="Left" Margin="763,636,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="1.662,0.935" Click="PrintDB_Click"/>

    </Grid>
